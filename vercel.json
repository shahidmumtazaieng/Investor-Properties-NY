{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "server/vercel-server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/**",
          "shared/**",
          "client/dist/**"
        ]
      }
    },
    {
      "src": "server/vercel-test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vercel-test",
      "dest": "/server/vercel-test.js"
    },
    {
      "src": "/api/test-vercel-db",
      "dest": "/server/test-vercel-db.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel-server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel-server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}